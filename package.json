{
  "name": "ai-voice-cloner",
  "version": "1.0.0",
  "description": "AI Voice Cloning application with Angular, NestJS, and Python",
  "scripts": {
    "start": "docker-compose up",
    "start:dev": "docker-compose up --build",
    "stop": "docker-compose down",
    "clean": "docker-compose down -v --remove-orphans",
    "frontend:dev": "cd frontend && npm run start",
    "backend:dev": "cd backend && npm run start:dev",
    "python:dev": "cd python-service && python app.py"
  },
  "keywords": ["ai", "voice-cloning", "tts", "angular", "nestjs", "python"],
  "author": "AI Voice Cloner Team",
  "license": "MIT",
  "devDependencies": {
    "nx": "^21.4.1"
  }
}